    <body>
        <div class="jumbotron">
            <h1 class="text-center">Money House</h1>
            <p class = "text-center">Resize this responsive page to see the effect!</p> 
        </div>

        <nav class="navbar navbar-inverse navbar-fixed-top"> <!--这是导航栏, 有基本的导航和搜索, 注册, 登录, 登出等-->
            <div class="container-fluid">
                <div class="navbar-header">
                    <a class="navbar-brand" href="#">MoneyHouse</a>
                </div>
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#">Home</a></li>
                    <li><a href="#">上海</a></li>
                    <li><a href="#">深圳</a></li> 
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            其他 
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="#">下拉表项1</a></li>
                            <li><a href="#">EJB</a></li>
                            <li><a href="#">Jasper Report</a></li>
                            <li class="divider"></li>
                            <li><a href="#">分离的链接</a></li>
                            <li class="divider"></li>
                            <li><a href="#">另一个分离的链接</a></li>
                        </ul>
                    </li>
                    <li>
                        <form class="navbar-form navbar-left" role="search">
                            <div class="form-group">
                                <input type="text" class="form-control" placeholder="Search">
                            </div>
                        </form>
                    </li>
                </ul>
                <ul class="nav navbar-nav navbar-right"> 
                    <li><img src="static/favicons/favicon.ico" class="img-circle"></li>
                    <li><a href="#"><span class="glyphicon glyphicon-user"></span> 注册</a></li> <!--未登陆时显示-->
                    <li><a href="#"><span class="glyphicon glyphicon-log-in"></span> 登录</a></li> <!--未登录时显示-->
                    <li><a href="#"><span class="glyphicon glyphicon-log-out"></span> 登出</a></li>  <!--已登录时显示-->
                </ul> 
            </div>
        </nav>
        <div class="row">
            {{ range .}}
            <div class="col-sm-4">
                <div class="thumbnail">
                    <!--<img alt="300x200" src="v3/default4.jpg" />-->
                    <div class="caption">
                        <h3> {{ .ShortName }} : {{ .StockCode }} </h3>
                        <p>{{ .StockCode }}</p>
                        <p>{{ .ShortName }}</p>
                        <p>{{ .TypeACode }}</p>
                        <p>{{ .TypeAShortName }}</p>
                        <p>{{ .TypeAOnSaleDate }}</p>
                        <p>{{ .TypeATotalValue }}</p>
                        <p>{{ .TypeACirculationValue }}</p>
                        <p> <a class="btn" href="#">View details »</a> </p>
                        <p>Nullam id dolor id nibh ultricies vehicula ut id elit.</p>
                        <p>
                        <a class="btn btn-primary" href="#">Action</a> 
                        <a class="btn" href="#">Action</a>
                        </p>
                    </div>
                </div>
            </div>
            {{end}}
        </div>
